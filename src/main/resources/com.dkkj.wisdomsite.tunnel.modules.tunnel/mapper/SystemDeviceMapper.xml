<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dkkj.wisdomsite.tunnel.modules.tunnel.mapper.SystemDeviceMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.dkkj.wisdomsite.tunnel.modules.tunnel.domain.SystemDevice">
                    <id column="id" property="id"/>
                    <result column="worksite_code" property="worksiteCode"/>
                    <result column="code" property="code"/>
                    <result column="device_name" property="deviceName"/>
                    <result column="type" property="type"/>
                    <result column="access_mode" property="accessMode"/>
                    <result column="ip_addr" property="ipAddr"/>
                    <result column="port" property="port"/>
                    <result column="status" property="status"/>
                    <result column="remarks" property="remarks"/>
                    <result column="create_time" property="createTime"/>
                    <result column="update_time" property="updateTime"/>
                    <result column="del_flag" property="delFlag"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, worksite_code, code, device_name, type, access_mode, ip_addr, port, status, remarks, create_time, update_time, del_flag
        </sql>

</mapper>